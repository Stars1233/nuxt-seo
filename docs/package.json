{
  "name": "nuxt-seo-docs",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "nuxi dev",
    "build": "npm -C ../ run build && nuxi build",
    "deploy": "NITRO_PRESET=netlify nuxt build && npx netlify deploy",
    "lint:docs": "markdownlint ./content && case-police 'content/**/*.md' *.md",
